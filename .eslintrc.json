{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "max-len": ["error", 100],
    "indent": ["error", 2],
    "require-jsdoc": "off",
    "linebreak-style": "off"
  }
}
